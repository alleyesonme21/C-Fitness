@{
  Layout = "_Layout";
}

@if (User.Identity.IsAuthenticated)
{
<h2>You are signed in as @User.Identity.Name</h2>
<hr />

    <p>Welcome back!</p>
    @* <p>@Html.ActionLink("Generate Workout", </p> *@
        @using (Html.BeginForm("LogOff", "Account"))
    {
        <img src="~/img/workout1.png" class="workout">
        <input type="submit" class="btn btn-default" value="Log out" />
    }
}
else
{
  <div class="row text-center">
    <div class="col-md-6">
      <p class="myLink">@Html.ActionLink("Register", "Register")</p>
    </div>
    <div class="col-md-6">
      <p class="myLink">@Html.ActionLink("Log in", "Login")</p>
    </div>
  </div>
    

}

<img src="~/img/workout1.png" class="workout">
